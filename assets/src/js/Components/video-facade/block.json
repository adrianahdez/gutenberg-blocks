{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gutenberg-blocks/video-facade",
	"version": "0.1.0",
	"title": "Video Facade",
	"category": "custom-gutenberg-blocks",
	"icon": "format-video",
	"description": "Block to display videos with facade",
	"example": {},
	"supports": {
		"html": false,
		"align": [ "wide", "full" ],
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"youtubeId": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "gutenberg-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": ""
}
